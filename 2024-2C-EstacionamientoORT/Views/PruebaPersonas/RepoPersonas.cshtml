@model IEnumerable<_2024_2C_EstacionamientoORT.Models.Persona>


<p>Las Personas en la lista del repositorio son</p>

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th>
                Nombre
            </th>

            <th>
                Apellido
            </th>
            <th>
                Dni
            </th>
            
        </tr>
    </thead>



    <tbody>
        @foreach (Persona persona in Model)
        {
            <tr>
                <td> @persona.Nombre</td>
                <td> @persona.Apellido</td>
                <td> @persona.Dni</td>
               
            </tr>

        }

    </tbody>
</table>